{
  "version": 2,
  "builds": [
    {
      "src": "api/search-image.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/search-image",
      "dest": "/api/search-image.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
} 